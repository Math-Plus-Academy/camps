<h1>Listing camp_interests</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Year</th>
      <th>Newsletter</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @camp_interests.each do |camp_interest| %>
      <tr>
        <td><%= camp_interest.name %></td>
        <td><%= camp_interest.email %></td>
        <td><%= camp_interest.year %></td>
        <td><%= camp_interest.newsletter %></td>
        <td><%= link_to 'Show', camp_interest %></td>
        <td><%= link_to 'Edit', edit_camp_interest_path(camp_interest) %></td>
        <td><%= link_to 'Destroy', camp_interest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Camp interest', new_camp_interest_path %>
