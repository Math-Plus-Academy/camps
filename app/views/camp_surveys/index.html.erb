<h1>Listing camp_surveys</h1>

<table>
  <thead>
    <tr>
      <th>Satisfaction</th>
      <th>Comment</th>
      <th>Improvements</th>
      <th>Parent first name</th>
      <th>Parent last name</th>
      <th>Student name</th>
      <th>Grade completed</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Contact time</th>
      <th>Preferred contact</th>
      <th>Contact fall</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @camp_surveys.each do |camp_survey| %>
      <tr>
        <td><%= camp_survey.satisfaction %></td>
        <td><%= camp_survey.comment %></td>
        <td><%= camp_survey.improvements %></td>
        <td><%= camp_survey.parent_first_name %></td>
        <td><%= camp_survey.parent_last_name %></td>
        <td><%= camp_survey.student_name %></td>
        <td><%= camp_survey.grade_completed %></td>
        <td><%= camp_survey.phone %></td>
        <td><%= camp_survey.email %></td>
        <td><%= camp_survey.contact_time %></td>
        <td><%= camp_survey.preferred_contact %></td>
        <td><%= camp_survey.contact_fall %></td>
        <td><%= link_to 'Show', camp_survey %></td>
        <td><%= link_to 'Edit', edit_camp_survey_path(camp_survey) %></td>
        <td><%= link_to 'Destroy', camp_survey, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Camp survey', new_camp_survey_path %>
